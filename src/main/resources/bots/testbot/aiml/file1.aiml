<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
	<category>
		<pattern>
			yummy yummy
		</pattern>
		<template>
			yummy in my tummy
		</template>
	</category>
	
	<category>
		<pattern>
			setyum *
		</pattern>
		<template>
			<set name="yum"> <star/> </set>
		</template>
	</category>

	
	<category>
		<pattern>WHAT DID YOU SAY</pattern>
		<template>
			<think><set name="response"> <response index="1"/></set></think>
			<condition name="response">
				<li value="unknown"> I didn't say anything</li>
				<li> I said, <response index="1"/> <split/> how's that?</li>
			</condition>
		</template>
	</category>
	
	<category>
		<pattern>say *</pattern>

		<template> ok  YES!!</template>
	</category>
</aiml>